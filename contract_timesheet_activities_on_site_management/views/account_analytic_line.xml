<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>      
		<record id="account_analytic_line_on_site" model="ir.ui.view">
			<field name="name">account.analytic.line.on.site</field>
			<field name="model">account.analytic.line</field>
			<field name="inherit_id" ref="analytic.view_account_analytic_line_tree"/>
			<field name="arch" type="xml">
				<xpath expr="(//field[@name='name'])" position="after">
					<field name="on_site"/>
				</xpath>
			</field>
		</record>

		<record id="account_analytic_line_form_on_site" model="ir.ui.view">
			<field name="name">account.analytic.line.form.on.site</field>
			<field name="model">account.analytic.line</field>
			<field name="inherit_id" ref="analytic.view_account_analytic_line_form" />
			<field name="arch" type="xml">
				<xpath expr="(//field[@name='name'])" position="after">
					<field name="on_site"/>
				</xpath>
			</field>
		</record>
	</data>
</openerp>
